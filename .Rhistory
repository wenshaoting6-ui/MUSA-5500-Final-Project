#| label: setup
import geopandas as gpd
import pandas as pd
import plotly.express as px
import plotly.graph_objects as go
# 1. Load the data prepared in the Jupyter Notebook
# Ensure you ran the 'Save Data' step in your notebook to create this file
try:
tracts = gpd.read_file("outputs/tract_summary.geojson")
except Exception as e:
print("Error loading data. Make sure 'outputs/tract_summary.geojson' exists.")
raise e
# 2. Ensure CRS is WGS84 (Latitude/Longitude) for web plotting
if tracts.crs != 'EPSG:4326':
tracts = tracts.to_crs(epsg=4326)
